    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "https://medium.com/uninow-engineering/build-a-react-native-theme-provider-using-hooks-2c303107109d",
    "android": {
      "package": "net.sytes.myblog"
    }
  }
}
