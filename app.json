{
  "expo": {
    "name": "BottomTabNavigator Theme Hooks",
    "slug": "bottomtabnavigator-with-theme",
    "version": "1.0.0",
    "sdkVersion": "38.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
